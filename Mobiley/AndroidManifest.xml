<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobiley"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".MobApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MobileEntry"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
             >
        </activity>
        <activity
            android:name=".SignUpActivity"
            android:label="@string/title_activity_sign_up" >
        </activity>
        <activity
            android:name=".NetworkProviders"
            android:label="Select your provider" >
        </activity>
        <activity
            android:name=".Plan"
            android:label="@string/title_activity_plan" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".NetworkProviders" />
        </activity>
        <activity
            android:name=".Cost"
            android:label="@string/title_activity_cost" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".NetworkProviders" />
        </activity>
        <activity
            android:name=".Final"
            android:label="@string/title_activity_final" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".NetworkProviders" />
        </activity>
        <activity
            android:name=".Last"
            android:label="@string/title_activity_last" >
        </activity>
        <activity
            android:name=".Payment"
            android:label="@string/title_activity_payment" >
        </activity>
        <activity
            android:name=".History"
            android:label="@string/title_activity_history" >
        </activity>
    </application>

</manifest>
